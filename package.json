{
    "name": "hoi4-mod-support",
    "displayName": "HOI4 Mod Support",
    "description": "Adds syntax highlighting for Hearts Of Iron 4 mod files.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "hoi4",
            "aliases": ["HOI4", "hoi4"],
            "extensions": [".txt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "hoi4",
            "scopeName": "source.hoi4",
            "path": "./syntaxes/hoi4.tmLanguage.json"
        }]
    }
}